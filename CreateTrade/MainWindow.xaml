            <GradientStop Color="#FF0298de" Offset="1"/>
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="BtnPlayPressed" EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF0d83b8" Offset="0"/>
            <GradientStop Color="#FF00618e" Offset="1"/>
        </LinearGradientBrush>

